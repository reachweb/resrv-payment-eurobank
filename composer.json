{
    "name": "reachweb/resrv-payment-eurobank",
    "description": "Eurobank (Greece) payment gateway for Statamic Resrv.",
    "keywords": [
        "Statamic",
        "Resrv"
    ],
    "homepage": "https://resrv.eu/",
    "license": "mit",
    "type": "statamic-addon",
    "authors": [
        {
            "name": "Iosif Chatzimichail",
            "email": "iosif@reach.gr",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@reach.gr"
    },
    "autoload": {
        "psr-4": {
            "Reach\\ResrvPaymentEurobank\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reach\\ResrvPaymentEurobank\\Tests\\": "tests"
        }
    },
    "require": {
        "statamic/cms": "^5.0",
        "reachweb/statamic-resrv": "^3.0 || dev-main"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Resrv Payment Eurobank",
            "description": "Resrv Payment Eurobank addon"
        },
        "laravel": {
            "providers": [
                "Reach\\ResrvPaymentEurobank\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
